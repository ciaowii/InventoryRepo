@model PL.MVC.IOBalance.Areas.AdminManagement.Models.SupplierSearchModel

<div class="modal fade" id="mdlsearchsupplier" tabindex="-1" role="dialog" aria-labelledby="mdlsearchsupplier" aria-hidden="true">
    <div class="modal-dialog">
        <h4 class="modal-title text-center">Search Supplier</h4>
    </div>
    <div class="modal-content">
        <div class="modal-body">
            <form class="form-horizontal" id="frmSearchSupplier">
                <div class="form-group">
                    <label for="" class="control-label col-sm-3">Supplier Code</label>
                    <div class="control-input col-sm-8">
                        @Html.TextBoxFor(m => m.SupplierCode,
                        new
                        {
                            @class = "form-control",
                            @id = "SupplierCode"
                        })
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="control-label col-sm-3">Supplier Name</label>
                    <div class="control-input col-sm-8">
                        @Html.TextBoxFor(m => m.SupplierCode,
                        new
                        {
                            @class = "form-control",
                            @id = "SupplierCode"
                        })
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
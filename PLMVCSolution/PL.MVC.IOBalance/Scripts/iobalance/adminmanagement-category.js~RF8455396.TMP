
$('body').on('click', '#clickme', function () {
    $elem = $(this);

    $.ajax({
        url: $elem.data('url'),
        type: 'POST',
        success: function (data) {
            $('#divResultContainer').show().html(data.alertMessage);
        }
    });
});

